@article{Lorenz2013,
	Author = {Lorenz, Jan and Paetzel, Fabian and Schweitzer, Frank},
	Doi = {10.1371/journal.pone.0054904},
	Local-Url = {/Users/Max/Mendeley/Lorenz, Paetzel, Schweitzer - 2013 - Redistribution spurs growth by using a portfolio effect on risky human capital.pdf},
	Issn = {1932-6203},
